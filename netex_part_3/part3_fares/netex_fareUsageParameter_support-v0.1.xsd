		<xsd:annotation>
			<xsd:documentation>Type for identifier of a USAGE PARAMETER PRICE.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="DiscountablePriceIdType"/>
	</xsd:simpleType>
