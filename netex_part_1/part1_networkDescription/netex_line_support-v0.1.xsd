	<!-- ==Group of Lines============================================================ -->
	<xsd:element name="GroupOfLinesRef" type="GroupOfLinesRefStructure" substitutionGroup="VersionOfObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a Group O fLines .</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="GroupOfLinesRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a GroupOfLines.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="GroupOfEntitiesRefStructure">
				<xsd:attribute name="ref" type="GroupOfLinesIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of a GroupOfLines.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:simpleType name="GroupOfLinesIdType">
		<xsd:annotation>
			<xsd:documentation>Type for Identifier of a GROUP OF LINES.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="GroupOfEntitiesIdType"/>
	</xsd:simpleType>
	<!-- ==Line============================================================ -->
	<xsd:complexType name="lineRefs_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a list of references to a LINE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="oneToManyRelationshipStructure">
				<xsd:sequence>
					<xsd:element ref="LineRef" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="LineIdType">
		<xsd:annotation>
			<xsd:documentation>Type for Identifier of a LINE.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="ObjectIdType"/>
	</xsd:simpleType>
	<xsd:element name="LineRef" type="LineRefStructure" substitutionGroup="VersionOfObjectRef">
		<xsd:annotation>
			<xsd:documentation>Reference to a LINE .</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="LineRefStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a LINE.</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="VersionOfObjectRefStructure">
				<xsd:attribute name="ref" type="LineIdType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identifier of a LINE.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
