	<!-- ======================================================================= -->
	<xsd:element name="OvertakingPossibility_" type="MeetingRestriction_VersionStructure" abstract="true" substitutionGroup="MeetingRestriction">
		<xsd:annotation>
			<xsd:documentation>Dummy suoertype for NetworkRestriction</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OvertakingPossibility" abstract="true" substitutionGroup="OvertakingPossibility_">
		<xsd:annotation>
			<xsd:documentation>NETWORK RESTRICTION specifying the possibility of overtaking or not over an INFRASTRUCTURE LINK</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="OvertakingPossibility_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup">
					 
							</xsd:group>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="NetworkRestrictionGroup"/>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:element name="ForVehicleTypeRef" type="VehicleTypeRefStructure" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Type of vehicle that may use forwards direction of INFRASTRUCTURE LINK</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="AgainstVehicleTypeRef" type="VehicleTypeRefStructure" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Type of vehicle that may use backwards direction of  INFRASTRUCTURE LINK</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:sequence>
