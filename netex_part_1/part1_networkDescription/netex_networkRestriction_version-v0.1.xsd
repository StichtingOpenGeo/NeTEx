					</xsd:sequence>
					<xsd:attribute name="id" type="RoadManoeuvreIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="RoadManoeuvre_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a Road MANOEUVRE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Manoeuvre_VersionStructure">
				<xsd:group ref="RoadLinkGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<xsd:element name="WireManoeuvre" abstract="false" substitutionGroup="Manoeuvre_">
		<xsd:annotation>
			<xsd:documentation>A MANOEUVRE on a wire network.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="WireManoeuvre_VersionStructure">
