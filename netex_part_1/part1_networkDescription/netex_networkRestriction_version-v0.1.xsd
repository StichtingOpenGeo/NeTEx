					</xsd:sequence>
					<xsd:attribute name="id" type="WireManoeuvreIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="WireManoeuvre_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a Wire MANOEUVRE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Manoeuvre_VersionStructure">
				<xsd:group ref="WireLinkGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<xsd:element name="MeetingRestriction" type="MeetingRestriction_VersionStructure" abstract="true" substitutionGroup="NetworkRestriction">
		<xsd:annotation>
			<xsd:documentation>A pair of INFRASTRUCTURE LINKs where vehicles of specified VEHICLE TYPEs are not allowed to meet.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="MeetingRestriction_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for MEETING RESTRICTION.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="InfrastructureLinkRestriction_VersionStructure">
				<xsd:group ref="MeetingRestrictionGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="MeetingRestrictionGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a  MEETING RESTRICTION.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ForVehicleTypeRef" type="VehicleTypeRefStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of vehicle that may use forwards direction of INFRASTRUCTURE LINK</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgainstVehicleTypeRef" type="VehicleTypeRefStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of vehicle that may use backwards direction of  INFRASTRUCTURE LINK</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
