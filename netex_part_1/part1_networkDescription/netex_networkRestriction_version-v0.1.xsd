	<!-- ===ENTIITY IN VERSION IN FRAME====Used in SERVICE FRAME)=================================================== -->
	<!-- ======================================================================= -->
	<xsd:group name="NetworkDescriptionInFrameGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for  NETWORK DESCRIPTION in frame.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="NetworkInfrastuctureInFrameGroup"/>
			<xsd:group ref="NetworkRestrictionInFrameGroup"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="NetworkRestrictionInFrameGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a NETWORK RESTRICTION.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="restrictions" type="networkRestrictionsInFrame_RelStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>restrictions on network</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="networkRestrictionsInFrame_RelStructure">
		<xsd:annotation>
			<xsd:documentation>Type for containment in frame of NETWORK RESTRICTION.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="containmentAggregationStructure">
				<xsd:sequence>
					<xsd:element ref="NetworkRestriction" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ===NETWORK RESTRICTIONs================================================= -->
	<xsd:element name="NetworkRestriction" type="NetworkRestriction_VersionStructure" abstract="true" substitutionGroup="DataManagedObject">
		<xsd:annotation>
			<xsd:documentation>A constraint on use of a network of INFRASTRUCTURE POINTs and INFRASTUCTURE LINKs.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="NetworkRestriction_VersionStructure" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Type for a  NETWORK RESTRICTION.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="DataManagedObjectStructure">
				<xsd:sequence>
					<xsd:group ref="NetworkRestrictionGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="NetworkRestrictionGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a NETWORK RESTRICTION.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Possible" type="xsd:boolean" default="true">
				<xsd:annotation>
					<xsd:documentation>Whether a NETWORK RESTRICTION is allowed or forbidden. Default is true</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="MultilingualString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of NETWORK RESTRICTION.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
	<!-- ====Point Restriction====================================================== -->
	<xsd:element name="VehicleTypeAtPoint" abstract="false" substitutionGroup="NetworkRestriction">
		<xsd:annotation>
			<xsd:documentation>NETWORK RESTRICTION. specifying whether a vehicle of a specified VEHICLE TYPE may visit a point.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="VehicleTypeAtPoint_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup">

							</xsd:group>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="NetworkRestrictionGroup"/>
						</xsd:sequence>
						<xsd:group ref="VehicleTypeAtPointGroup"/>
					</xsd:sequence>
					<xsd:attribute name="id" type="VehicleTypeAtPointIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="VehicleTypeAtPoint_VersionStructure" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Type for a VEHICLE TYPE AT POINT.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="NetworkRestriction_VersionStructure">
				<xsd:group ref="VehicleTypeAtPointGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="VehicleTypeAtPointGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a VEHICLE TYPE AT POINT.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ForVehicleTypeRef" type="VehicleTypeRefStructure">
				<xsd:annotation>
					<xsd:documentation>Type of VEHICLE to which NETWORK RESTRICTION applies.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Capacity" type="NumberOfVehicles" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number of vehicles allowed at a point at a given time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ====Link Restrictions===================================================== -->
	<xsd:element name="InfrastructureLinkRestriction" abstract="true" substitutionGroup="NetworkRestriction">
		<xsd:annotation>
			<xsd:documentation>a NETWORK RESTRICTION on movement between any two network links.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="InfrastructureLinkRestriction_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup">
				 
							</xsd:group>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="NetworkRestrictionGroup"/>
						</xsd:sequence>
					</xsd:sequence>
					<xsd:attribute name="id" type="NetworkRestrictionIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="InfrastructureLinkRestriction_VersionStructure" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Type for a NETWORK LINK RESTRICTION.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="NetworkRestriction_VersionStructure"/>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<xsd:element name="Manoeuvre_" type="NetworkRestriction_VersionStructure" abstract="true" substitutionGroup="NetworkRestriction">
		<xsd:annotation>
			<xsd:documentation>Dummy supertype for manoeuvres </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Manoeuvre" abstract="true" substitutionGroup="NetworkRestriction">
		<xsd:annotation>
			<xsd:documentation>A specification of an move for a certain type of vehicle. It specifies from which INFRASTRUCTURE LINK to which other (adjacent) INFRASTRUCTURE LINK a certain can or cannot VEHICLE TYPE cannot proceed, due to physical restrictions</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="Manoeuvre_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup">
			 
							</xsd:group>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="NetworkRestrictionGroup"/>
						</xsd:sequence>
						<xsd:group ref="ManoeuvreGroup"/>
					</xsd:sequence>
					<xsd:attribute name="id" type="ManoeuvreIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="Manoeuvre_VersionStructure" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Type for a MANOEUVRE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="InfrastructureLinkRestriction_VersionStructure">
				<xsd:group ref="ManoeuvreGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="ManoeuvreGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a  MANOEUVRE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="VehicleTypeRef" type="VehicleTypeRefStructure" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifier of TYPE OF VEHICLE making MANOEUVRE.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
	<xsd:group name="RoadLinkGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a Road MANOEUVRE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FromLinkRef" type="RoadLinkRefStructure">
				<xsd:annotation>
					<xsd:documentation>Restriction applies to movements starting from INFRASTRUCTURE LINK identified by this Reference.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ToLinkRef" type="RoadLinkRefStructure">
				<xsd:annotation>
					<xsd:documentation>Restriction applies to movements ending on INFRASTRUCTURE LINK identified by this reference.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="RailwayLinkGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a RAILWAY MANOEUVRE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FromLinkRef" type="RailwayLinkRefStructure">
				<xsd:annotation>
					<xsd:documentation>Restriction applies to movements starting from INFRASTRUCTURE LINK identified by this Reference.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ToLinkRef" type="RailwayLinkRefStructure">
				<xsd:annotation>
					<xsd:documentation>Restriction applies to movements ending on INFRASTRUCTURE LINK identified by this reference.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="WireLinkGroup">
		<xsd:annotation>
			<xsd:documentation>Elements for a Wire MANOEUVRE.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FromLinkRef" type="WireLinkRefStructure">
				<xsd:annotation>
					<xsd:documentation>Restriction applies to movements starting from INFRASTRUCTURE LINK identified by this Reference.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ToLinkRef" type="WireLinkRefStructure">
				<xsd:annotation>
					<xsd:documentation>Restriction applies to movements ending on INFRASTRUCTURE LINK identified by this reference.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
	<xsd:element name="RailwayManoeuvre" abstract="false" substitutionGroup="Manoeuvre_">
		<xsd:annotation>
			<xsd:documentation>A MANOEUVRE on a rail network.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="RailwayManoeuvre_VersionStructure">
					<xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="DataManagedObjectGroup">
	 
							</xsd:group>
						</xsd:sequence>
						<xsd:sequence>
							<xsd:group ref="NetworkRestrictionGroup"/>
						</xsd:sequence>
						<xsd:group ref="ManoeuvreGroup"/>
						<xsd:group ref="RailwayLinkGroup"/>
					</xsd:sequence>
					<xsd:attribute name="id" type="RailwayManoeuvreIdType"/>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="RailwayManoeuvre_VersionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for a RAILWAY MANOEUVRE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Manoeuvre_VersionStructure">
				<xsd:group ref="RailwayLinkGroup"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<xsd:element name="RoadManoeuvre" abstract="false" substitutionGroup="Manoeuvre_">
		<xsd:annotation>
			<xsd:documentation>A MANOEUVRE on a road network.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="RoadManoeuvre_VersionStructure">
