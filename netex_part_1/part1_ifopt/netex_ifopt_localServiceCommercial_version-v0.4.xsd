		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ServiceList" type="CommunicationServiceListOfEnumerations" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Communication services available.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
