<?xml version="1.0" encoding="iso-8859-1"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:core="http://www.govtalk.gov.uk/core" xmlns="http://www.netex.org.uk/netex" xmlns:netex="http://www.netex.org.uk/netex" targetNamespace="http://www.netex.org.uk/netex" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.1" id="netex_organisation_version">	<xsd:include schemaLocation="../netex_utility/netex_utility_contact-v0.1.xsd"/>	<xsd:include schemaLocation="../netex_responsibility/netex_responsibilitySet_version-v0.1.xsd"/>	<xsd:include schemaLocation="netex_organisation_support-v0.1.xsd"/>	<xsd:include schemaLocation="netex_zone_version-v0.1.xsd"/>	<xsd:annotation>		<xsd:appinfo>			<Metadata xmlns="http://www.govtalk.gov.uk/CM/gms-xs">				<Aggregation>main schema</Aggregation>				<Audience>e-service developers</Audience>				<Coverage>Europe</Coverage>				<Creator>Drafted for version 1.0 CEN TC278 WG3 SG6 Editor Nicholas Knowles, Kizoom. mailto:schemer@netex.org.uk</Creator>				<Date>					<Created>2010-09-04</Created>				</Date>				<Date>					<Modified>2011-02-05</Modified>				</Date>				<Date>					<Modified>2010-11-05</Modified>				</Date>				<Description>					<p>NeTEx - Network Exchange.This subschema defines ORGANISATION  types.</p>				</Description>				<Format>					<MediaType>text/xml</MediaType>					<Syntax>http://www.w3.org/2001/XMLSchema</Syntax>					<Description>XML schema, W3C Recommendation 2001</Description>				</Format>				<Identifier>{http://www.netex.org.uk/schemas/0.4/netex}netex_utility_types-v0.2.xsd</Identifier>				<Language>[ISO 639-2/B] ENG</Language>				<Publisher>Kizoom Software Ltd, 16 High Holborn, London WC1V 6BX</Publisher>				<Rights>Unclassified                            <Copyright>CEN, Crown Copyright 2006, 2007, 2011</Copyright>				</Rights>				<Source>					<ul>						<li>Derived from the Transmodel, VDV, TransXChange, NaPTAN, NOPTIS, BISON and Trident standards.</li>					</ul>				</Source>				<Status>Version 1.0 Draft for approval</Status>				<Subject>					<Category>Arts, recreation and travel, Tourism, Travel (tourism), Transport,Air transport, Airports,Ports and maritime transport, Ferries (marine),Public transport, Bus services, Coach services, Bus stops and stations,Rail transport, Railway stations and track, Train services, Underground trains,Business and industry, Transport, Air transport , Ports and maritime transport, Public transport,Rail transport, Roads and road transport</Category>					<Project>Cen TC278 WG3 SG6</Project>				</Subject>				<Title>NeTEx Network Exchange - ORGANISATION. types.</Title>				<Type>Standard</Type>			</Metadata>		</xsd:appinfo>		<xsd:documentation>ORGANISATION types for NeTEx</xsd:documentation>	</xsd:annotation>	<!-- ===ENTIITY IN VERSION IN FRAME====Used in SERVICE FRAME)=================================================== -->	<xsd:complexType name="organisationsInFrame_RelStructure">		<xsd:annotation>			<xsd:documentation>Type for containment in frame of ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="containmentAggregationStructure">				<xsd:sequence>					<xsd:element ref="Organisation_" maxOccurs="unbounded"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:complexType name="departmentsInFrame_RelStructure">		<xsd:annotation>			<xsd:documentation>Type for containment in frame of DEPARTMENTs</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="containmentAggregationStructure">				<xsd:sequence>					<xsd:element ref="Department" maxOccurs="unbounded"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:element name="Organisation_" type="DataManagedObjectStructure" abstract="true" substitutionGroup="DataManagedObject">		<xsd:annotation>			<xsd:documentation>Dummy supertyep fpr organisation</xsd:documentation>		</xsd:annotation>	</xsd:element>	<xsd:element name="Organisation" abstract="true" substitutionGroup="Organisation_">		<xsd:annotation>			<xsd:documentation>An legally incorporated body associated with any aspect of the transport system.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:restriction base="Organisation_VersionStructure">					<xsd:sequence>						<xsd:sequence>							<xsd:group ref="DataManagedObjectGroup"/>						</xsd:sequence>						<xsd:sequence>							<xsd:group ref="OrganisationGroup">								<xsd:annotation>									<xsd:documentation>Details for Organisation</xsd:documentation>								</xsd:annotation>							</xsd:group>						</xsd:sequence>					</xsd:sequence>					<xsd:attribute name="id" type="OrganisationIdType"/>				</xsd:restriction>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="Organisation_VersionStructure" abstract="true">		<xsd:annotation>			<xsd:documentation>Type for an ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DataManagedObjectStructure">				<xsd:sequence>					<xsd:group ref="OrganisationGroup">						<xsd:annotation>							<xsd:documentation>Details for Organisation</xsd:documentation>						</xsd:annotation>					</xsd:group>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="OrganisationGroup">		<xsd:annotation>			<xsd:documentation>Elements for an ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:group ref="OrganisationCodeGroup"/>			<xsd:group ref="OrganisationNameGroup"/>			<xsd:group ref="OrganisationPropertiesGroup"/>			<xsd:element name="Status" type="xsd:boolean" default="true" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Whether the ORGANISATION is active. Default is true.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ValidityPeriod" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Period when the ORGANISATION is valid</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:complexContent>						<xsd:extension base="SimpleValidityConditionStructure"/>					</xsd:complexContent>				</xsd:complexType>			</xsd:element>			<xsd:element name="parts" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Parts of the ORGANISATION.</xsd:documentation>				</xsd:annotation>				<xsd:complexType>					<xsd:complexContent>						<xsd:extension base="organisationParts_RelStructure"/>					</xsd:complexContent>				</xsd:complexType>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="OrganisationCodeGroup">		<xsd:annotation>			<xsd:documentation>Code details for an ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="PublicCode" type="xsd:normalizedString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Public code to use for operator</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element ref="PrivateCode" minOccurs="0"/>			<xsd:element name="CompanyNumber" type="xsd:normalizedString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Company registration number including Country prefix</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="OrganisationNameGroup">		<xsd:annotation>			<xsd:documentation>Name details for an ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Name" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The name of the ORGANISATION.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ShortName" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>A short name of the ORGANISATION.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="LegalName" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The legal name of the ORGANISATION, if different from Name</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="TradingName" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>The Trading name of the ORGANISATION given to the Public - If different from Name or Legal Name.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<xsd:group name="OrganisationPropertiesGroup">		<xsd:annotation>			<xsd:documentation>Properties for an ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Description" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Description of ORGANISATION.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Remarks" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Further remarks about ORGANISATION.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element ref="Locale" minOccurs="0"/>			<xsd:element name="ContactDetails" type="ContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Contact details for ORGANISATION for Publ;ic use.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="PrivateContactDetails" type="ContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Contact details for ORGANISATION  Private use.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="OrganisationType" type="OrganisationTypeListOfEnumerations" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Type of ORGANISATION.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="operatorTypes" type="typeOfOrganisationRefs_RelStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Classification of of OPERATOR. Used for arbitrary documnetation -  .</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= -->	<xsd:complexType name="organisationParts_RelStructure">		<xsd:annotation>			<xsd:documentation>Type for a list of ORGANISATION PARTs</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="containmentAggregationStructure">				<xsd:choice maxOccurs="unbounded">					<xsd:element ref="OrganisationPartRef"/>					<xsd:element ref="OrganisationPart"/>				</xsd:choice>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="OrganisationPart" abstract="false" substitutionGroup="DataManagedObject">		<xsd:annotation>			<xsd:documentation>A named subdivision of an ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:restriction base="OrganisationPart_VersionStructure">					<xsd:sequence>						<xsd:sequence>							<xsd:group ref="DataManagedObjectGroup"/>						</xsd:sequence>						<xsd:sequence>							<xsd:group ref="OrganisationPartGroup"/>						</xsd:sequence>					</xsd:sequence>					<xsd:attribute name="id" type="OrganisationPartIdType"/>				</xsd:restriction>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="OrganisationPart_VersionStructure" abstract="false">		<xsd:annotation>			<xsd:documentation>Type for an ORGANISATION PART.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DataManagedObjectStructure">				<xsd:sequence>					<xsd:group ref="OrganisationPartGroup"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="OrganisationPartGroup">		<xsd:annotation>			<xsd:documentation>Elements for an ORGANISATION PART.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Name" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Name of ORGANISATION PART.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ContactDetails" type="ContactStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Contact details for ORGANISATION for Publ;ic use.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element ref="TypeOfOrganisationPartRef" minOccurs="0"/>			<xsd:element name="administrativeZones" type="administrativeZones_RelStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Zones managed by ORGANISATION PART.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= -->	<xsd:complexType name="departments_RelStructure">		<xsd:annotation>			<xsd:documentation>Type for a list of  DEPARTMENTs.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="containmentAggregationStructure">				<xsd:choice maxOccurs="unbounded">					<xsd:element ref="DepartmentRef"/>					<xsd:element ref="Department"/>				</xsd:choice>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="Department" abstract="false" substitutionGroup="DataManagedObject">		<xsd:annotation>			<xsd:documentation>Department of an ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:restriction base="Department_VersionStructure">					<xsd:sequence>						<xsd:sequence>							<xsd:group ref="DataManagedObjectGroup"/>						</xsd:sequence>						<xsd:sequence>							<xsd:group ref="DepartmentGroup"/>						</xsd:sequence>					</xsd:sequence>					<xsd:attribute name="id" type="DepartmentIdType"/>				</xsd:restriction>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="Department_VersionStructure" abstract="false">		<xsd:annotation>			<xsd:documentation>Type for a DEPARTMENT.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DataManagedObjectStructure">				<xsd:sequence>					<xsd:group ref="DepartmentGroup"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="DepartmentGroup">		<xsd:annotation>			<xsd:documentation>Elements for a DEPARTMENT.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element name="Name" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Name of DEPARTMENT.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="ShortName" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Name of DEPARTMENT.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Description" type="MultilingualString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Name of DEPARTMENT.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Location" type="LocationStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Coordinates of DEPARTMENT.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="PublicCode" type="xsd:normalizedString" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Additional public code used for department</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= -->	<xsd:complexType name="administrativeZones_RelStructure">		<xsd:annotation>			<xsd:documentation>Type for a list of ADMINISTRATIVE ZONE.s</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="containmentAggregationStructure">				<xsd:choice maxOccurs="unbounded">					<xsd:element ref="AdministrativeZoneRef"/>					<xsd:element ref="AdministrativeZone"/>				</xsd:choice>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="AdministrativeZone" abstract="false" substitutionGroup="Zone">		<xsd:annotation>			<xsd:documentation>A ZONE relating to the management responsibilities of an ORGANISATION. For example to allocate bus stop identiifers for a region.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:restriction base="AdministrativeZone_VersionStructure">					<xsd:sequence>						<xsd:sequence>							<xsd:group ref="DataManagedObjectGroup"/>						</xsd:sequence>						<xsd:sequence>							<xsd:group ref="GroupOfEntitiesGroup"/>						</xsd:sequence>						<xsd:sequence>							<xsd:group ref="ZoneGroup"/>						</xsd:sequence>						<xsd:sequence>							<xsd:group ref="AdministrativeZoneGroup"/>						</xsd:sequence>					</xsd:sequence>					<xsd:attribute name="id" type="AdministrativeZoneIdType"/>				</xsd:restriction>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="AdministrativeZone_VersionStructure" abstract="false">		<xsd:annotation>			<xsd:documentation>Type for an ADMINISTRATIVE ZONE.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="Zone_VersionStructure">				<xsd:sequence>					<xsd:group ref="AdministrativeZoneGroup"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="AdministrativeZoneGroup">		<xsd:annotation>			<xsd:documentation>Elements for an ADMINISTRATIVE ZONE.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:element ref="OrganisationRef" minOccurs="0"/>			<xsd:element name="responsibilities" type="responsibilitySets_RelStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>RESPONSIBILITY SETs allocated to ADMINISTRATIVE ZONE.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="namespaces" type="namespaceAssignments_RelStructure" minOccurs="0">				<xsd:annotation>					<xsd:documentation>NAMESPACEs belonging to ADMINISTRATIVE ZONE.</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= -->	<xsd:complexType name="namespaceAssignments_RelStructure">		<xsd:annotation>			<xsd:documentation>Type for a list of NAME SPACE ASSIGNMENT.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="strictContainmentAggregationStructure">				<xsd:sequence maxOccurs="1">					<xsd:element name="NamespaceAssignment" type="NamespaceAssignment_VersionedChildStructure" maxOccurs="unbounded">						<xsd:annotation>							<xsd:documentation>Assignment of  use of a NAMESPACE witjh responsibility for managing data within a given ZONE.</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:element name="NamespaceAssignment" abstract="false" substitutionGroup="VersionedChild">		<xsd:annotation>			<xsd:documentation>Assignment of  use of a NAMESPACE witjh responsibility for managing data within a given ZONE.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:restriction base="NamespaceAssignment_VersionedChildStructure">					<xsd:sequence>						<xsd:sequence>							<xsd:group ref="VersionedChildGroup"/>						</xsd:sequence>						<xsd:sequence>							<xsd:group ref="NamespaceAssignmentGroup"/>						</xsd:sequence>					</xsd:sequence>					<xsd:attribute name="id" type="NamespaceAssignmentIdType" use="optional"/>				</xsd:restriction>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="NamespaceAssignment_VersionedChildStructure" abstract="false">		<xsd:annotation>			<xsd:documentation>Type for a NAME SPACE ASSIGNMENT.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="VersionedChildStructure">				<xsd:sequence>					<xsd:group ref="NamespaceAssignmentGroup"/>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<xsd:group name="NamespaceAssignmentGroup">		<xsd:annotation>			<xsd:documentation>Elements for a NAME SPACE ASSIGNMENT.</xsd:documentation>		</xsd:annotation>		<xsd:sequence>			<xsd:choice>				<xsd:element name="NamespaceRef" type="NamespaceIdType"/>				<xsd:element ref="Namespace"/>			</xsd:choice>			<xsd:element ref="AdministrativeZoneRef" minOccurs="0"/>			<xsd:element name="StartDate" type="xsd:date" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Start date for effectiveness of NAMESPACE ASSIGNMENT.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="EndDate" type="xsd:date" minOccurs="0">				<xsd:annotation>					<xsd:documentation>End date for effectiveness of NAMESPACE ASSIGNMENT. Date is INCLUSIVE</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="NameOfClass" type="NameOfClass" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Name of class whose names  are being assigned</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="CodePrefix" type="xsd:normalizedString" minOccurs="0" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation>Code prefix assigned uniquely to zone. Used to allocate new ids  If NAMESPACE also specified, used within namespace. Only this Zone may allocate codes beginning with these prefixes</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="Description" type="xsd:string" minOccurs="0">				<xsd:annotation>					<xsd:documentation>Description of NAME SPACE ASSIGNMENT</xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:group>	<!-- ======================================================================= -->	<xsd:element name="TypeOfOrganisation" abstract="false" substitutionGroup="TypeOfValue">		<xsd:annotation>			<xsd:documentation>Classification of an ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:restriction base="TypeOfOrganisation_ValueStructure">					<xsd:sequence>						<xsd:sequence>							<xsd:group ref="DataManagedObjectGroup"/>						</xsd:sequence>						<xsd:sequence>							<xsd:group ref="TypeOfValueGroup"/>						</xsd:sequence>					</xsd:sequence>					<xsd:attribute name="id" type="TypeOfOrganisationIdType"/>				</xsd:restriction>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="TypeOfOrganisation_ValueStructure" abstract="false">		<xsd:annotation>			<xsd:documentation>Type for a TYPE OF ORGANISATION.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="TypeOfValue_VersionStructure"/>		</xsd:complexContent>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:element name="TypeOfOrganisationPart" abstract="false" substitutionGroup="TypeOfValue">		<xsd:annotation>			<xsd:documentation>Classification of an ORGANISATION PART.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:restriction base="TypeOfOrganisationPart_ValueStructure">					<xsd:sequence>						<xsd:sequence>							<xsd:group ref="DataManagedObjectGroup"/>						</xsd:sequence>						<xsd:sequence>							<xsd:group ref="TypeOfValueGroup"/>						</xsd:sequence>					</xsd:sequence>					<xsd:attribute name="id" type="TypeOfOrganisationPartIdType"/>				</xsd:restriction>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="TypeOfOrganisationPart_ValueStructure" abstract="false">		<xsd:annotation>			<xsd:documentation>Type for a TYPE OF ORGANISATION PART.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="TypeOfValue_VersionStructure"/>		</xsd:complexContent>	</xsd:complexType>	<!-- ======================================================================= -->	<xsd:simpleType name="OrganisationTypeListOfEnumerations">		<xsd:annotation>			<xsd:documentation>List of Allowed values for ORGANISATION TYPE</xsd:documentation>		</xsd:annotation>		<xsd:list itemType="OrganisationTypeEnumeration"/>	</xsd:simpleType>	<!-- ======================================================================= -->	<xsd:element name="OrganisationView" substitutionGroup="DerivedView">		<xsd:annotation>			<xsd:documentation>Simplified view of ORGANISATION. All data except the identifier will be derived through the refernced to the Organisation.</xsd:documentation>		</xsd:annotation>		<xsd:complexType>			<xsd:complexContent>				<xsd:extension base="Organisation_DerivedViewStructure"/>			</xsd:complexContent>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="Organisation_DerivedViewStructure">		<xsd:annotation>			<xsd:documentation>Type for an ORGANISATION VIEW.</xsd:documentation>		</xsd:annotation>		<xsd:complexContent>			<xsd:extension base="DerivedViewStructure">				<xsd:sequence>					<xsd:element ref="OrganisationRef" minOccurs="0"/>					<xsd:group ref="OrganisationNameGroup"/>					<xsd:element name="ContactDetails" type="ContactStructure" minOccurs="0">						<xsd:annotation>							<xsd:documentation>Contact details for ORGANISATION for Publ;ic use.</xsd:documentation>						</xsd:annotation>					</xsd:element>				</xsd:sequence>			</xsd:extension>		</xsd:complexContent>	</xsd:complexType>	<!-- ======================================================================= --></xsd:schema>