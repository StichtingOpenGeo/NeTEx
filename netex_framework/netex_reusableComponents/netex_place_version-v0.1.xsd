<xsd:schema xmlns="http://www.netex.org.uk/netex" xmlns:netex="http://www.netex.org.uk/netex" xmlns:siri="http://www.siri.org.uk/siri" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.netex.org.uk/netex" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.1" id="netex_place_version">
	<xsd:include schemaLocation="netex_place_support-v0.1.xsd"/>
	<xsd:include schemaLocation="../netex_genericFramework/netex_zone_version-v0.1.xsd"/>
	<xsd:include schemaLocation="../netex_genericFramework/netex_zoneProjection_version-v0.1.xsd"/>
	<xsd:include schemaLocation="netex_mode_version-v0.3.xsd"/>
