	<!-- ==== Entity ================================================================ -->
	<xsd:element name="Entity" type="EntityStructure" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Any data instance to be managed in an operational Version Management System. When several DATA SOURCEs coexist (multimodality and/or interoperability), an ENTITY  has to be related to a given DATA SYSTEM in which it is defined.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="EntityStructure" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Type for ENTITY.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="nameOfClass" type="NameOfClass" use="optional">
			<xsd:annotation>
				<xsd:documentation>Name of Class of the ENTITY. Allows reflection. Fixed for each ENTITY type</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="id" type="ObjectIdType" use="optional"/>
	</xsd:complexType>
	<!-- ====Namespace =================================================================== -->
	<xsd:element name="Namespace" type="NamespaceStructure" abstract="false">
		<xsd:annotation>
			<xsd:documentation>A system for uniquely identifying objects. Used for the distributed managejment of objects from many different sources.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="NamespaceStructure" abstract="false">
		<xsd:annotation>
			<xsd:documentation>Type for NAMESPACE.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="EntityStructure">
				<xsd:sequence>
					<xsd:element name="Xmlns" type="NamespaceIdType">
						<xsd:annotation>
							<xsd:documentation>Prefix used to identify NAMESPACE, eg 'napt', 'oda', 'ratp' etc</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="XmlnsUrl" type="xsd:anyURI" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>NAMESPACE  path. The URL associated which with the NAMESPACE  defines a unique system for identifying objects within the NAMESPACE.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Description" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Description of NAMESPACE</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="dataSourceRef" type="DataSourceIdType"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ==== Entity Class =================================================================== -->
	<xsd:element name="ClassInRepository" type="ClassInRepositoryStructure">
		<xsd:annotation>
			<xsd:documentation>Class of Entity. This is a metaclass that allows the type of the Entity to be Referenced in applications, for example in a filter. Each first class NeTEx object should add a subtype in order to be visible.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ClassInRepositoryStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Class of Entity. This is a metaclass that allows the type of the Entity to be Referenced in applications, for exammple in a filter. Each first class NeTEx object should add a csuptype in order to be visible.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="nameOfClass"/>
	</xsd:complexType>
</xsd:schema>
