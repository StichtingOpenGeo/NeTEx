		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:A_to_B_ticket_concession  0.90
		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:A_to_C_ticket_concession  2.50
		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:B_to_B_ticket_concession  0.40
		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:B_to_C_ticket_concession  1.50
		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:C_to_C_ticket_concession  0.50
	PRICE GROUP  myfares:PriceGroup:travelCard_standard
		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:A_to_A_travelCard_standard 0.40
		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:A_to_B_travelCard_standard  0.90
		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:A_to_C_travelCard_standard  2.60	
		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:B_to_B_travelCard_standard 0.40	
		DISTANCE MATRIX ELEMENT  PRICE :myfares:DistanceMatrixElementPrice:B_to_C_travelCard_standard  1.90	
